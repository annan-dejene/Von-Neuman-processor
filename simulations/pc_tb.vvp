    %store/vec4 v0x101569b20_0, 0, 1;
    %wait E_0x101568d10;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x101569720_0, 0, 8;
    %wait E_0x101568d10;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x101569720_0, 0, 8;
    %wait E_0x101568d10;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x101569720_0, 0, 8;
