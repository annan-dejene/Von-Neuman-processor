    %load/vec4 v0x101568ef0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x101560d10_0, 0;
    %jmp T_1.1;
T_1.0 ;
